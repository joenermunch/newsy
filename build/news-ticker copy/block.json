{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "newsy-blocks/news-ticker",
  "version": "0.1.0",
  "title": "News Ticker",
  "category": "widgets",
  "icon": "megaphone",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "textdomain": "news-ticker",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "title": {
      "type": "string",
      "default": null,
      "source": "html",
      "selector": "h1"
    },
    "subtitle": {
      "type": "string",
      "default": null,
      "source": "html",
      "selector": "span"
    },
    "posts": {
      "type": "array"
    }
  }
}